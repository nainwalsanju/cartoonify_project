{
  "builds": [
    {"src": "cartoonify_project/wsgi.py","use": "@vercel/python","config": { "maxLambdaSize": "15mb", "runtime": "python" }},
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.py", "use": "@vercel/python" }
    
    ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "cartoonify_project/wsgi.py"
      }
  ]
}
