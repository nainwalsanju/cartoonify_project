{
  "builds": [{
      "src": "cartoonify_project/wsgi.py",
      "use": "@vercel/python"
  },
  {
    "functions": {
      "api/**/*.py": {
        "excludeFiles": "**/*.html"
      }
    }
  }],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "cartoonify_project/wsgi.py"
      }
  ]
}
